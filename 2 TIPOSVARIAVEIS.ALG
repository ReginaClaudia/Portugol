Algoritmo "Tiposvariaveis"
Var
   Modelo: Caractere
   Ano : Inteiro
   Valor: Real
   IpvaPago: Logico
   SeguroPago: Logico
Inicio
   Modelo <- "Gol"
   Ano <- 2010
   Valor <- 20500.14
   IpvaPago <- FALSO
   SeguroPago <- VERDADEIRO
   
   Escreval(Modelo)
   Escreval (Ano)
   Escreval (Valor)
   Escreval (IpvaPago)
   Escreval (SeguroPago)
Fimalgoritmo